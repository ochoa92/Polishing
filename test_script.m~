clear all;
clc;
close all;


c = 0.5;
x = -15:0.001:15;
sigmoid = 1./(1+exp(-c*x));

tau_ext = 0:0.001:10;
tau_m = sigmoid * tau_ext;

figure(1)
hold on 
plot(x,sigmoid)

grid on